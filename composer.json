{
    "name": "thelogicstudio/experian-businesses-php",
    "description": "Unofficial PHP SDK for Experian's Businesses API",
    "keywords": [
        "experian",
        "php",
        "sdk",
        "api"
    ],
    "homepage": "https://logicstudio.nz",
    "license": "mit",
    "authors": [
        {
            "name": "Ian Simpson"
        }
    ],
    "require": {
        "php": ">=5.5",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2 | ^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "squizlabs/php_codesniffer": "~2.6",
        "friendsofphp/php-cs-fixer": "~2.12"
    },
    "autoload": {
        "psr-4": { "TheLogicStudio\\ExperianBusinessesPHP\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "TheLogicStudio\\ExperianBusinessesPHP\\" : "test/" }
    }
}
